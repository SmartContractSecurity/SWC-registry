{
    "title": "SWC config",
    "type": "object",
    "required": ["description", "issues"],
    "properties": {
        "description": {
            "type": "string"
        },
        "issues": {
            "title": "Issue set",
            "type": "array",
            "items": {
                "title": "Issue",
                "type": "object",
                "required": ["id", "count"],
                "properties": {
                    "id": {
                        "type": "string"
                    },
                    "count": {
                        "type": "number"
                    },
                    "locations": {
                        "type": "array",
                        "items": {
                            "bytecode_offsets": {
                                "type": "object"
                            },
                            "line_numbers": {
                                "type": "object"
                            }
                        }
                    }
                }
            }
        }
    }
}